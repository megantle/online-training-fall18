let myvariable = 'Hello World'
myvariable
"Hello World"

const myothervairbale = 'Hello' (use when to not accidentally change the variable)
myothervariable
"Hello"

myvariable = 'otherthing'
"otherthing"

typeof "helloworld"
"string"

typeof 1
"number"

typeof true
"boolean"

typeof null (null means that there's nothing there)
"object"

typeof {}
"object"

typeof []
"object"

typeof function () { return 5 }
"function"
{'banana': 'yellow', 'orange': 'orange'}
let a = {'banana': 'yellow', 'orange': 'orange'}
a.banana
"yellow"

1 === 1 (to make them equal, like in math; can't use single '=' that's for assigning)
